<div class="col-2 element text-center mb-4" data-id="<%= element._id %>" data-slug="<%= element.slug %>">
    <div class="element-container">
        <a href="<%= data.element + element._id %>">
            <div class="element-content">
                <img class="d-block mx-auto" src="<%= cloudinary.url(data.path + element.slug + ".png", {fetch_format: "auto", quality: "auto", secure: true}) %>" alt="<%= __(element.name) %>" width="128" />
                <h2 class="font-weight-bold h5 mt-4"><%= __(element.name) %></h2>
            </div>
        </a>
    </div>
    <div class="element-tools">
        <div class="row">
            <div class="col-12 element-delete mx-auto">
                <a href="<%= data.delete %>">
                    <span>Delete element</span>
                </a>
            </div>
        </div>
    </div>
</div>
