<!-- Modal -->
<div class="modal fade" id="<%= data.id %>" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"><%= data.title %></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p class="mb-0 modal-content-to-replace"></p>
            </div>
            <div class="modal-footer">
                <button id="modal-response-submit" type="button" class="submit custom-theme small theme-3 mx-auto" data-dismiss="modal">Close</button>
                <button type="button" class="dismiss custom-theme small theme-3 mx-auto" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
