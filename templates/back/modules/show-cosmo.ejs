<div class="col-2 cosmo system text-center mb-4" data-id="<%= cosmo._id %>" data-slug="<%= cosmo.slug %>">
    <div class="cosmo-container system-container">
        <a href="../back/update-cosmo/:<%= cosmo._id %>">
            <div class="cosmo-content system-content">
                <img class="d-block mx-auto" src="<%= cloudinary.url("cosmos/" + cosmo.slug + ".png", {fetch_format: "auto", quality: "auto", secure: true}) %>" alt="<%= cosmo.name %>" width="128" />
                <h2 class="font-weight-bold h5 mt-4"><%= __(cosmo.name) %></h2>
            </div>
        </a>
    </div>
    <div class="cosmo-tools system-tools">
        <div class="row">
            <div class="col-12 cosmo-delete system-delete mx-auto">
                <a href="../api/cosmo/delete">
                    <span>Delete cosmo</span>
                </a>
            </div>
        </div>
    </div>
</div>
